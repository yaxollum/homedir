#!/bin/bash

if [ $# -ne 1 ]
then
    echo Expected one argument.
    exit
fi

set -e
mkdir $1
cd $1
cat > main.cc << EOF
#include <bits/stdc++.h>

using namespace std;

typedef int64_t i64;
typedef double f64;
typedef int8_t flag;

const i64 MOD = 1000000007;
const i64 INF = 0x3f3f3f3f3f3f3f3f;
const i64 NEG_INF = -INF;

int main() {
  cin.sync_with_stdio(0);
  cin.tie(0);

  return 0;
}
EOF
